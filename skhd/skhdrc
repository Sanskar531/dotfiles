# focus window
cmd - j : fish /Users/$(whoami)/.config/skhd/window_cycling_backward.fish
cmd - k : fish /Users/$(whoami)/.config/skhd/window_cycling_forward.fish

# swap managed window
cmd - return : yabai -m window --swap west 

cmd + shift - c : yabai -m window --close && fish /Users/$(whoami)/.config/skhd/window_cycling_backward.fish
cmd + shift - c : yabai -m window --close && fish /Users/$(whoami)/.config/skhd/window_cycling_backward.fish
# layouts
cmd - space : fish /Users/$(whoami)/.config/skhd/layout_cycling.fish
cmd - b : open -a "Google Chrome" -n

cmd + shift - return : /Applications/Kitty.app/Contents/MacOS/kitty ~

# float / unfloat window and center on screen
cmd + shift - t : yabai -m window --toggle float --grid 4:4:1:1:2:2
# in bash
cmd - s : screencapture -iU `date -U`

cmd + shift - 1 : yabai -m window --space 1
cmd + shift - 2 : yabai -m window --space 2
cmd + shift - 3 : yabai -m window --space 3
cmd + shift - 4 : yabai -m window --space 4
cmd + shift - 5 : yabai -m window --space 5
cmd + shift - 6 : yabai -m window --space 6
cmd + shift - 7 : yabai -m window --space 7
cmd + shift - 8 : yabai -m window --space 8
cmd + shift - 9 : yabai -m window --space 9
